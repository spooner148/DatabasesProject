    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_Survey_has_User_User2`
    FOREIGN KEY (`Email`)
    REFERENCES `Database_Project`.`Person` (`Email`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `Database_Project`.`Survey_has_Question`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `Database_Project`.`Survey_has_Question` (
  `S_Num` INT NOT NULL,
  `Q_Num` INT NOT NULL,
  `Email` VARCHAR(45) NOT NULL,
  `Q_Completed` TINYINT NULL,
  PRIMARY KEY (`S_Num`, `Q_Num`, `Email`),
  INDEX `fk_Survey_has_Question_Question1_idx` (`Q_Num` ASC) VISIBLE,
  INDEX `fk_Survey_has_Question_Survey1_idx` (`S_Num` ASC) VISIBLE,
  INDEX `fk_Survey_has_Question_User1_idx` (`Email` ASC) VISIBLE,
  CONSTRAINT `fk_Survey_has_Question_Survey1`
    FOREIGN KEY (`S_Num`)
    REFERENCES `Database_Project`.`Survey` (`S_Num`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_Survey_has_Question_Question1`
    FOREIGN KEY (`Q_Num`)
    REFERENCES `Database_Project`.`Question` (`Q_Num`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_Survey_has_Question_User1`
    FOREIGN KEY (`Email`)
    REFERENCES `Database_Project`.`Person` (`Email`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


